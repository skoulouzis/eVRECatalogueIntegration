        cerif:has_definitionSource  "http://lu.com/odlis/odlis_N.cfm#newsstory" ;
        cerif:has_example           "called Newspaper Article in CASRAI Dictionary" ;
        cerif:has_roleExpression    "is a" ;
        cerif:has_term              "Newsclipping" ;
        cerif:has_termSource        "CERIF Task Group; CASRAI Dictionary" .

